<h1>teachers</h1>

<% @teachers.each do |teacher| %>
    <div class="card teacher-card" style="width: 18rem;">
    <img src="https://images.unsplash.com/photo-1503945438517-f65904a52ce6?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1740&q=80" class="card-img-top" alt="...">
        <div class="card-body">
            <h3><%= teacher.first_name %> <%= teacher.last_name %></h3>

            <p class="card-text"><%= teacher.profil %></p>
        </div>
    </div>
<% end %>
